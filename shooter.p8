pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
t = 0
ship = {
  x = 60,
  y = 60,
  sp = 1,
  h=3,
  p=0,
  box={
    x1=0,
    y1=0,
    x2=7,
    y2=7,
  },
  t=0,
  imm=false
}
bullets = {}
enemies = {}
explosions = {}
for i=1,8 do
  add(enemies, {
    sp=17,
    m_x=30+i*10,
    m_y=60-i*8,
    x=-64,
    y=-64,
    r=12,
    box={
      x1=0,
      y1=0,
      x2=7,
      y2=7,
    }
  })
end

function abs_box(s)
  local box = {}
  box.x1 = s.x + s.box.x1
  box.y1 = s.y + s.box.y1
  box.x2 = s.x + s.box.x2
  box.y2 = s.y + s.box.y2
  return box
end

function _init()
  start()
end

function start()
  _update = update_game
  _draw = draw_game
end

function game_over()
  _update = update_over
  _draw = draw_over
end

function update_over()
end

function draw_over()
  cls()
  print("game over", 50, 50)
end

function update_game()
  t = (t + 1) % 6
  ship.sp = (t < 3) and 1 or 2

  for b in all(bullets) do
    b.x += b.dx
    b.y += b.dy
    if b.x < 0 or 128 < b.x or
       b.y < 0 or 127 < b.y then
      del(bullets, b)
    end
    for e in all(enemies) do
      if collide(e, b) then
        del(enemies, e)
        -- explode
        add(explosions, {
          x=e.x,
          y=e.y,
          t=0,
        })
        ship.p += 1
      end
    end
  end

  if ship.imm then
    ship.t += 1

    if ship.t >= 30 then
      ship.imm = false
      ship.t = 0
    end
  end

  for e in all(enemies) do
    e.x = e.m_x + e.r * cos(time())
    e.y = e.m_y + e.r * sin(time())

    if collide(e, ship) and not ship.imm then
      ship.imm = true
      ship.h -= 1
      if ship.h < 1 then
        game_over()
      end
    end
  end

  for ex in all(explosions) do
    ex.t += 1
    if ex.t == 13 then
      del(explosions, ex)
    end
  end

  if btn(0) then ship.x -= 1 end
  if btn(1) then ship.x += 1 end
  if btn(2) then ship.y -= 1 end
  if btn(3) then ship.y += 1 end
  if btnp(4) then fire() end
end

function fire()
  add(bullets, {
    sp = 3,
    x = ship.x,
    y = ship.y,
    dx = 0,
    dy = -3,
    box={
      x1=3,
      y1=0,
      x2=4,
      y2=2,
    }
  })
end

function collide(a, b)
  local box_a = abs_box(a)
  local box_b = abs_box(b)

  if box_a.x1 > box_b.x2 or
     box_a.y1 > box_b.y2 or
     box_b.x1 > box_a.x2 or
     box_b.y1 > box_a.y2 then
    return false
  end

  return true
end

function draw_game()
  cls()
  print(time())

  if not ship.imm or t < 3 then
    spr(ship.sp, ship.x, ship.y)
  end

  for b in all(bullets) do
    spr(b.sp, b.x, b.y)
  end
  for e in all(enemies) do
    spr(e.sp, e.x, e.y)
  end
  for ex in all(explosions) do
    circ(ex.x, ex.y, ex.t, 8 + ex.t%3)
  end
  for i=1,4 do
    if i <= ship.h then
      spr(33, 80+6*i, 6)
    else
      spr(34, 80+6*i, 6)
    end
  end
end
